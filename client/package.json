        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.2.12",
        "@vue/eslint-config-airbnb": "^5.0.2",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-vue": "^7.18.0",
        "node-sass": "^4.12.0",
        "sass-loader": "^8.0.2",
        "vue-loader-v16": "^16.0.0-beta.5.4"
